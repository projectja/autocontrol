import { OnInit } from '@angular/core';
export declare class NgxFontAwesomeComponent implements OnInit {
    name: string;
    size: string;
    animation: boolean;
    rotate: number;
    flip: string;
    fixed: boolean;
    border: boolean;
    customClass: string;
    classes: string;
    constructor();
    ngOnInit(): void;
    /**
     * getClasses()
     * Function will return Options in class form
     */
    getClasses(): string;
}
